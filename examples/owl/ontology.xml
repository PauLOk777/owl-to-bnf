<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Ontology>
    <Prefix IRI="http://example.com/myOntology#" name="myOnt"/>
    <Import>http://example.com/someOtherOntology</Import>
    <Declaration>
        <Class abbreviatedIRI="myOnt:Horse"/>
    </Declaration>
    <Declaration>
        <Class abbreviatedIRI="myOnt:Human"/>
    </Declaration>
    <Declaration>
        <Class IRI="#Grass"/>
    </Declaration>
    <Declaration>
        <Class IRI="#Water"/>
    </Declaration>
    <Declaration>
        <ObjectProperty IRI="#eats"/>
    </Declaration>
    <Declaration>
        <ObjectProperty abbreviatedIRI="myOnt:drink"/>
    </Declaration>
    <SubClassOf>
        <ObjectProperty>
            <ObjectProperty IRI="#eats"/>
            <Class IRI="#Grass"/>
        </ObjectProperty>
        <Class abbreviatedIRI="myOnt:Horse"/>
    </SubClassOf>
    <SubClassOf>
        <ObjectProperty>
            <ObjectProperty abbreviatedIRI="myOnt:drink"/>
            <Class IRI="#Water"/>
        </ObjectProperty>
        <Class abbreviatedIRI="myOnt:Horse"/>
    </SubClassOf>
    <SubClassOf>
        <ObjectProperty>
            <ObjectProperty abbreviatedIRI="myOnt:drink"/>
            <Class IRI="#Water"/>
        </ObjectProperty>
        <Class abbreviatedIRI="myOnt:Human"/>
    </SubClassOf>
</Ontology>
